{
  "scripts": {
    "prettier": "prettier --write .*js *js workbench/static/workbench/* 'timer/**/*.{js,scss}' 'absences/**/*.{js,scss}'",
    "check": "eslint workbench/static/workbench/ timer/**/*.js && prettier --list-different 'timer/**/*.{js,scss}' 'absences/**/*.{js,scss}'",
    "eslint": "eslint workbench/static/workbench/ timer/**/*.js absences/**/*.js",
    "build-bootstrap": "webpack -p --config webpack.config.bootstrap.js",
    "dev": "webpack-dev-server --hot",
    "prod": "NODE_ENV=production webpack -p --bail"
  },
  "eslintIgnore": [
    "workbench/static/workbench/lib"
  ],
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.5.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "fh-webpack-config": "^1.0.5",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.3",
    "react-redux": "^7.2.0",
    "react-select": "^3.1.0",
    "redux": "^4.0.5",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "url-loader": "^4.0.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "bracketSpacing": false,
    "semi": false
  }
}
